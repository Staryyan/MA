    // CSS
    link(href='../templates/css/app.min.1.css', rel='stylesheet')
    link(href='../templates/css/app.min.2.css', rel='stylesheet')
    block stylesheets

append header
    header#header.clearfix(data-current-skin='blue')
        ul.header-inner
            li#menu-trigger(data-trigger='#sidebar')
                .line-wrap
                    .line.top
                    .line.center
                    .line.bottom
            li.logo.hidden-xs
                a My Achievement
    section#main
        aside#sidebar.sidebar.c-overflow
            .profile-menu
                a(href='')
                    .profile-pic
                        img(src='img/profile-pics/1.jpg', alt='')
                    .profile-info
                        | My Achievement
                        i.zmdi.zmdi-caret-down
                ul.main-menu
                    li
                        a(href='/profile')
                            i.zmdi.zmdi-account
                            |  View Profile
                    li
                        a(href='/logout')
                            i.zmdi.zmdi-time-restore
                            |  Logout
            ul.main-menu
                li
                    a(href='/admin_home')
                        i.zmdi.zmdi-home
                        |  Home
                li.sub-menu
                    a(href='')
                        i.zmdi.zmdi-view-list
                        | Users
                    ul
                        li.sub-menu
                            a(href='')
                                i.zmdi.zmdi-assignment-account
                                |  All Users
                                ul
                                    li
                                        a(href='/admin_viewUsers_student')
                                            |  student
                                    li
                                        a(href='/admin_viewUsers_TA')
                                            |  TA
                        li
                            a(href='/admin_importUsers')
                                i.zmdi.zmdi-edit
                                |  Import Users
                li.menu
                    a(href='/scores')
                        i.zmdi.zmdi-view-list
                        |  Scores
                li
                    a(href='/admin_publishHomework')
                        i.zmdi.zmdi-calendar
                        |  Publish
        section#content
            block content

