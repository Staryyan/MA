    body
        header#header.clearfix(data-current-skin='blue')
            ul.header-inner
                li#menu-trigger(data-trigger='#sidebar')
                    .line-wrap
                        .line.top
                        .line.center
                        .line.bottom
                li.logo.hidden-xs
                    a(href='/admin_home') Material Admin
        section#main
            aside#sidebar.sidebar.c-overflow
                .profile-menu
                    a(href='')
                        .profile-pic
                            img(src='img/profile-pics/1.jpg', alt='')
                        .profile-info
                            | Malinda Hollaway
                            i.zmdi.zmdi-caret-down
                    ul.main-menu
                        li
                            a(href='/profile')
                                i.zmdi.zmdi-account
                                |  View Profile
                        li
                            a(href='/logout')
                                i.zmdi.zmdi-time-restore
                                |  Logout
                ul.main-menu
                    li
                        a(href='/admin_home')
                            i.zmdi.zmdi-home
                            |  Home
                    li.sub-menu
                        a(href='')
                            i.zmdi.zmdi-view-list
                            | Users
                        ul
                            li.sub-menu
                                a(href='')
                                    i.zmdi.zmdi-assignment-account
                                    |  All Users
                                    ul
                                        li
                                            a(href='/admin_viewUsers_student')
                                                |  student
                                        li
                                            a(href='/admin_viewUsers_TA')
                                                |  TA
                            li
                                a(href='/admin_importUsers')
                                    i.zmdi.zmdi-edit
                                    |  Import Users
                    li.menu
                        a(href='/admin_scores')
                            i.zmdi.zmdi-view-list
                            |  Scores
                    li
                        a(href='/admin_publishHomework')
                            i.zmdi.zmdi-calendar
                            |  Publish
            section#content
                block content

        footer#footer
            | Copyright Â© 2015 Material Admin
            ul.f-menu
                li
                    a(href='') Home
                li
                    a(href='') Dashboard
                li
                    a(href='') Reports
                li
                    a(href='') Support
                li
                    a(href='') Contact
        // Page Loader
        .page-loader
            .preloader.pls-blue
                svg.pl-circular(viewbox='25 25 50 50')
                    circle.plc-path(cx='50', cy='50', r='20')
                p Please wait...
