extends layout

append style
    link(href='../templates/vendors/bower_components/animate.css/animate.min.css', rel='stylesheet')
    link(href='../templates/vendors/bower_components/bootstrap-sweetalert/lib/sweet-alert.css', rel='stylesheet')
    link(href='../templates/vendors/bower_components/material-design-iconic-font/dist/css/material-design-iconic-font.min.css', rel='stylesheet')
    link(href='../templates/vendors/bower_components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css', rel='stylesheet')
    link(href='../templates/vendors/bootgrid/jquery.bootgrid.min.css', rel='stylesheet')
    link(href='../templates/vendors/bower_components/bootstrap-select/dist/css/bootstrap-select.css', rel='stylesheet')
    link(href='../templates/vendors/bower_components/nouislider/distribute/jquery.nouislider.min.css', rel='stylesheet')
